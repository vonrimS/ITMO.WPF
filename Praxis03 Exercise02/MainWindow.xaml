<Window x:Class="Praxis03_Exercise02.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praxis03_Exercise02"
        mc:Ignorable="d"
        Title="MainWindow" Height="280" Width="443">
    <Grid>
        <TextBox x:Name="txtBox" HorizontalAlignment="Left" Height="23" Margin="181,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <StackPanel ButtonBase.Click="StackPanel_Click_1"  HorizontalAlignment="Left" Height="100" Margin="28,22,0,0" VerticalAlignment="Top" Width="100">
            <Button x:Name="butAdd" Content="x+x"/>
            <Button x:Name="butMult" Content="x*x"/>
            <Button x:Name="butSqrt" Content="sqrt(x)"/>
        </StackPanel>

    </Grid>
</Window>
